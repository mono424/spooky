{
  "name": "@spooky/query-builder",
  "version": "1.0.0",
  "description": "Type-safe query builder for SurrealDB with relationship support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "dev:example": "pnpm build:watch",
    "dev": "tsdown --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "surrealdb",
    "query-builder",
    "typescript",
    "type-safe"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "surrealdb": "2.0.0-alpha.15"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsdown": "^0.12.4",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}