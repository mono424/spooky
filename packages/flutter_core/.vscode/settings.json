{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.dart_tool/**": true,
        "**/build/**": true,
        "**/.pub-cache/**": true,
        "**/example/**": true,
        "**/test/**": true,
        "**/android/**": true,
        "**/ios/**": true,
        "**/macos/**": true,
        "**/linux/**": true,
        "**/windows/**": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.dart_tool": true,
        "**/build": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.dart_tool": true,
        "**/build": true,
        "**/example": true,
        "**/test": true,
        "**/android": true,
        "**/ios": true,
        "**/macos": true,
        "**/linux": true,
        "**/windows": true
    },
    "dart.analysisExcludedFolders": [
        "**/.dart_tool",
        "**/build",
        "**/.pub-cache",
        "**/node_modules",
        "**/example",
        "**/test",
        "**/android",
        "**/ios",
        "**/linux",
        "**/macos",
        "**/windows"
    ]
}
