{
  "name": "@example/schema",
  "version": "1.0.0",
  "description": "Database schema and types for SurrealDB",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "dev": "pnpm build &&docker-compose up",
    "dev:example": "pnpm dev",
    "build": "pnpm build:ts && pnpm build:dart && pnpm build:surql",
    "build:ts": "syncgen --format typescript --input ./src/schema.surql --output ../app-solid/src/schema.gen.ts",
    "build:dart": "syncgen --format dart --input ./src/schema.surql --output ../client-dart/src/db/models.dart",
    "build:surql": "syncgen --input ./src/schema.surql --output ./.spooky/remote.gen.surql --modules-dir ../../packages/surrealism-modules",
    "db:reset": "docker-compose down -v && pnpm dev"
  },
  "keywords": [
    "database",
    "schema",
    "surrealdb"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "syncgen": "workspace:*"
  }
}
